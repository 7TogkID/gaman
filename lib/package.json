{
  "name": "gaman",
  "version": "0.0.11",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs_ts": "tsc --project tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "build:esm_ts": "tsc --project tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "build:cjs": "node build-cjs.js",
    "build:esm": "node build-esm.js",
    "build": "npm run clean  && npm run build:cjs && npm run build:esm && tsc"
  },
  "author": "angga7togk",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/7TogkID/gaman"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/formidable": "^3.4.5",
    "esbuild": "^0.25.5",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3"
  },
  "peerDependencies": {
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "formidable": "^3.5.4",
    "glob": "^11.0.3"
  }
}
