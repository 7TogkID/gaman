{
  "name": "gaman",
  "version": "0.0.47",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.20.8"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsx ./build/build.ts",
    "test": "vitest run",
    "test:push": "npm run gen:exports && npm run build && yalc push",
    "gen:exports": "tsx build/generate-exports.ts",
    "release": "npm run gen:exports && npm run build && npm publish",
    "release:beta": "npm run gen:exports && npm run build && npm publish --tag beta",
    "lint:src": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix"
  },
  "author": "angga7togk",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/7TogkID/gaman.git"
  },
  "bugs": {
    "url": "https://github.com/7TogkID/gaman/issues"
  },
  "homepage": "https://gaman.7togk.id",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@gaman/cli": "0.0.12",
    "@types/ws": "^8.18.1",
    "busboy": "^1.6.0",
    "cookie": "^1.0.2",
    "dotenv": "^17.2.0",
    "ws": "^8.18.2"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./nunjucks": {
      "types": "./dist/types/integration/nunjucks/index.d.ts",
      "import": "./dist/integration/nunjucks/index.js",
      "require": "./dist/cjs/integration/nunjucks/index.js"
    },
    "./ejs": {
      "types": "./dist/types/integration/ejs/index.d.ts",
      "import": "./dist/integration/ejs/index.js",
      "require": "./dist/cjs/integration/ejs/index.js"
    },
    "./static": {
      "types": "./dist/types/integration/static/index.d.ts",
      "import": "./dist/integration/static/index.js",
      "require": "./dist/cjs/integration/static/index.js"
    },
    "./session": {
      "types": "./dist/types/integration/session/index.d.ts",
      "import": "./dist/integration/session/index.js",
      "require": "./dist/cjs/integration/session/index.js"
    },
    "./cors": {
      "types": "./dist/types/middleware/cors/index.d.ts",
      "import": "./dist/middleware/cors/index.js",
      "require": "./dist/cjs/middleware/cors/index.js"
    },
    "./basic-auth": {
      "types": "./dist/types/middleware/basic-auth/index.d.ts",
      "import": "./dist/middleware/basic-auth/index.js",
      "require": "./dist/cjs/middleware/basic-auth/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "nunjucks": [
        "./dist/types/integration/nunjucks/index"
      ],
      "ejs": [
        "./dist/types/integration/ejs/index"
      ],
      "static": [
        "./dist/types/integration/static/index"
      ],
      "session": [
        "./dist/types/integration/session/index"
      ],
      "cors": [
        "./dist/types/middleware/cors/index"
      ],
      "basic-auth": [
        "./dist/types/middleware/basic-auth/index"
      ]
    }
  },
  "yalcSig": "2813fe20afa29f8913e16062355f067b"
}
