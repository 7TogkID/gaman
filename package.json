{
  "name": "gaman",
  "version": "0.0.33",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs_ts": "tsc --project tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "build:esm_ts": "tsc --project tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "build:cjs": "node build-cjs.js",
    "build:esm": "node build-esm.js",
    "build": "npm run clean  && npm run build:cjs && npm run build:esm && tsc",
    "build:bun": "npm run clean && bun ./build/build.ts",
    "test": "npm run build:bun && bun --watch test/src/main"
  },
  "author": "angga7togk",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/7TogkID/gaman"
  },
   "bugs": {
    "url": "https://github.com/7TogkID/gaman/issues"
  },
  "peerDependencies": {
    "@gaman/cli": "0.0.7"
  },
  "devDependencies": {
    "@gaman/cors": "^0.0.2",
    "@gaman/ejs": "^0.0.8",
    "@gaman/static": "^0.0.2",
    "@types/bun": "^1.2.17",
    "@types/busboy": "^1.5.4",
    "@types/ejs": "^3.1.5",
    "@types/ws": "^8.18.1",
    "arg": "^5.0.2",
    "busboy": "^1.6.0",
    "cookie": "^1.0.2",
    "ejs": "^3.1.10",
    "gaman": "^0.0.23",
    "glob": "^11.0.3",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "ws": "^8.18.2"
  }
}
